{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "/get",
      "function": "get"
    },
    {
      "source": "/insert",
      "function": "insert"
    },
    {
      "source": "/delete",
      "function": "delete"
    }]
  }
}
